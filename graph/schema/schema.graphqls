type Todo {
  id: ID!
  text: String!
  done: Boolean!
  userId: ID!
}

enum Category {
  TODAY
  TOMORROW
  EVER
}

type User {
  id: ID!
  name: String!
}

type Query {
  todos(category: Category): [Todo!]!
}

input NewTodo {
  text: String!
  userId: String!
}

type Mutation {
  createTodo(input: NewTodo!): Todo!
}
